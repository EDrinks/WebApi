{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "logs\\log-{Date}.txt"
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "EventStore": {
    "IPAddress": "127.0.0.1",
    "Port": 1113
  }
}
